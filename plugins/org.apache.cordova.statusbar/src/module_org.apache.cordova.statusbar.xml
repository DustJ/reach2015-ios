<?xml version="1.0" encoding="UTF-8"?>
<project name="module_org.apache.cordova.statusbar" default="compile.module.org.apache.cordova.statusbar">
  <dirname property="module.org.apache.cordova.statusbar.basedir" file="${ant.file.module_org.apache.cordova.statusbar}"/>
  
  <property name="module.jdk.home.org.apache.cordova.statusbar" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.org.apache.cordova.statusbar" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.org.apache.cordova.statusbar" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.org.apache.cordova.statusbar" value="-encoding UTF-8 -source 1.6 ${compiler.args}"/>
  
  <property name="org.apache.cordova.statusbar.output.dir" value="${module.org.apache.cordova.statusbar.basedir}/../../../out/production/org.apache.cordova.statusbar"/>
  <property name="org.apache.cordova.statusbar.testoutput.dir" value="${module.org.apache.cordova.statusbar.basedir}/../../../out/test/org.apache.cordova.statusbar"/>
  
  <path id="org.apache.cordova.statusbar.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="org.apache.cordova.statusbar.module.production.classpath">
    <path refid="${module.jdk.classpath.org.apache.cordova.statusbar}"/>
    <path refid="library.classes.classpath"/>
    <pathelement location="${cordovalib.output.dir}"/>
  </path>
  
  <path id="org.apache.cordova.statusbar.runtime.production.module.classpath">
    <pathelement location="${org.apache.cordova.statusbar.output.dir}"/>
    <path refid="library.classes.classpath"/>
    <path refid="cordovalib.runtime.production.module.classpath"/>
  </path>
  
  <path id="org.apache.cordova.statusbar.module.classpath">
    <path refid="${module.jdk.classpath.org.apache.cordova.statusbar}"/>
    <pathelement location="${org.apache.cordova.statusbar.output.dir}"/>
    <path refid="library.classes.classpath"/>
    <pathelement location="${cordovalib.testoutput.dir}"/>
    <pathelement location="${cordovalib.output.dir}"/>
  </path>
  
  <path id="org.apache.cordova.statusbar.runtime.module.classpath">
    <pathelement location="${org.apache.cordova.statusbar.testoutput.dir}"/>
    <pathelement location="${org.apache.cordova.statusbar.output.dir}"/>
    <path refid="library.classes.classpath"/>
    <path refid="cordovalib.runtime.module.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.org.apache.cordova.statusbar">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.org.apache.cordova.statusbar">
    <patternset refid="excluded.from.module.org.apache.cordova.statusbar"/>
  </patternset>
  
  <path id="org.apache.cordova.statusbar.module.sourcepath">
    <dirset dir="${module.org.apache.cordova.statusbar.basedir}">
      <include name="android"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.org.apache.cordova.statusbar" depends="compile.module.org.apache.cordova.statusbar.production,compile.module.org.apache.cordova.statusbar.tests" description="Compile module org.apache.cordova.statusbar"/>
  
  <target name="compile.module.org.apache.cordova.statusbar.production" depends="register.custom.compilers,compile.module.cordovalib" description="Compile module org.apache.cordova.statusbar; production classes">
    <mkdir dir="${org.apache.cordova.statusbar.output.dir}"/>
    <javac2 destdir="${org.apache.cordova.statusbar.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.org.apache.cordova.statusbar}/javac">
      <compilerarg line="${compiler.args.org.apache.cordova.statusbar}"/>
      <bootclasspath refid="org.apache.cordova.statusbar.module.bootclasspath"/>
      <classpath refid="org.apache.cordova.statusbar.module.production.classpath"/>
      <src refid="org.apache.cordova.statusbar.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.org.apache.cordova.statusbar"/>
    </javac2>
    
    <copy todir="${org.apache.cordova.statusbar.output.dir}">
      <fileset dir="${module.org.apache.cordova.statusbar.basedir}/android">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.org.apache.cordova.statusbar.tests" depends="register.custom.compilers,compile.module.org.apache.cordova.statusbar.production" description="compile module org.apache.cordova.statusbar; test classes" unless="skip.tests"/>
  
  <target name="clean.module.org.apache.cordova.statusbar" description="cleanup module">
    <delete dir="${org.apache.cordova.statusbar.output.dir}"/>
    <delete dir="${org.apache.cordova.statusbar.testoutput.dir}"/>
  </target>
</project>